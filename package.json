{
  "name": "@jaaromy/s3-sync",
  "version": "0.2.2",
  "description": "Watch a local file, directory, or glob for changes and sync them to an s3 bucket (or bucket and path).",
  "main": "app.js",
  "bin": {
    "s3-sync": "./app.js"
  },
  "dependencies": {
    "chokidar": "^2.0.4",
    "pm2": "^3.2.2",
    "shelljs": "^0.8.2",
    "yargs": "^12.0.2"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sync": "pm2 start ./app.js --no-autorestart --name s3-sync --",
    "stop": "pm2 delete s3-sync",
    "logs": "pm2 logs",
    "status": "pm2 status",
    "monitor": "pm2 monit"
  },
  "author": "Jaaromy Zierse",
  "repository": "https://github.com/Jaaromy/s3-sync.git",
  "license": "MIT"
}
